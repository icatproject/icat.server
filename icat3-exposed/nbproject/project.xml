<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <buildExtensions xmlns="http://www.netbeans.org/ns/ant-build-extender/1">
            <extension file="jaxws-build.xml" id="jaxws"/>
        </buildExtensions>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>icat3-exposed</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <included-library dirs="200">reference.icat3-core.jar</included-library>
            <included-library dirs="200">reference.icat3-jaxb.jar</included-library>
            <included-library dirs="200">reference.icat3-data.jar</included-library>
            <included-library dirs="200">file.reference.eclipselink-2.0.2.jar</included-library>
            <included-library dirs="200">file.reference.eclipselink-javax.persistence-2.0.jar</included-library>
            <included-library dirs="100">reference.icat3-logging.dist</included-library>
            <included-library dirs="200">file.reference.log4j-1.2.14.jar</included-library>
            <included-library dirs="200">file.reference.javaee.jar</included-library>
            <included-library dirs="200">reference.icat3-user-ansto.jar</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <junit-version xmlns="http://www.netbeans.org/ns/junit/1" value="junit4"/>
        <libraries xmlns="http://www.netbeans.org/ns/ant-project-libraries/1">
            <definitions>../Libraries\nblibraries.properties</definitions>
        </libraries>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>icat3-core</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>icat3-data</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>icat3-jaxb</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>icat3-logging</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
            <reference>
                <foreign-project>icat3-user-ansto</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
